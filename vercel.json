{
    "builds": [
      { "src": "src/**/*.html", "use": "@vercel/static" },
      { "src": "src/**/*.css", "use": "@vercel/static" },
      { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
      { "src": "/(.*).css", "dest": "/src/$1.css" },
      { "src": "/(.*)", "dest": "/src/$1" }
    ]
  }