{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.css",
      "use": "@vercel/static"
    }
    
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "static":{
    "directory":"./assest"
  }
}
