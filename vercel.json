{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.css",
      "use": "@vercel/static",
      "config":{
        "distDir":"dist"
      }
    }
    
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
  
}
