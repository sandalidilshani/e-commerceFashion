{
  "name": "e-commercefashion",
  "version": "1.0.0",
  "main": "tailwind.config.js",
  "scripts": {
    "test": "",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "build": "npx tailwindcss -i ./src/input.css -o ./src/output.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "builds": [
    { "src": "src/**/*.html", "use": "@vercel/static" },
    { "src": "src/**/*.css", "use": "@vercel/static" },
    { "src": "/assest/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assest/(.*)", "dest": "/assest/$1" },
    { "src": "/(.*).css", "dest": "/src/$1.css" },
    { "src": "/(.*)", "dest": "/src/$1" }
  ]
}
